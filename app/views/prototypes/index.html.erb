<main class="main">
  <div class="inner">
    <%= render partial: "prototype", collection: @prototypes %>
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、<%= link_to current_user.name, user_path(current_user) %>さん
      </div>
    <% else %>
      <div>ユーザーはログインしていません</div>
    <% end %>  
  </div>
</main>